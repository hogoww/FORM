;indent
(setf indent-string "     ")

;logic operators
(setf orOp '(or))
(setf andOp '(and))
(setf implyOp '(-> imply))
(setf equalOp '(<=> equal))
(setf notOp '(~ not))
(setf existsOp '(exists))
(setf forallOp '(forall))

;Class related to each operator
(setf orClass "Or")
(setf andClass "And")
(setf implyClass "Imply")
(setf equalClass "Equal")
(setf notClass "Not")
(setf existsClass "Exists")
(setf forallClass "ForAll")

